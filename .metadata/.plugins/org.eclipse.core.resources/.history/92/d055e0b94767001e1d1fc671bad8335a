package Ex2;

public class Point {
	double x;
	double y;
	
	Point(double x, double y) {
		this.x = x;
		this.y = y;
	}
	Point () {
		this.x = 0;
		this.y = 0;
	}
	
	double distance(Point p) {
		double x2 = p.x;
		double y2 = p.y;
		double resultat = Math.pow((x - x2), 2) + Math.pow((y-y2), 2);
		
	}
}
